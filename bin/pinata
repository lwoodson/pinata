#!/usr/bin/env ruby
require 'pathname'
file_path = Pathname.new(__FILE__).realpath
path = File.expand_path(File.join('..', '..', 'lib'), file_path)
$:.unshift(path)

require 'pinata'
project = Pinata::Project.new
project.whack_it!
reporter = Pinata::Reporters::Stream.new(project)
reporter.make_report
